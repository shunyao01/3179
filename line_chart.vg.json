{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Number of meteorite reported over the years",
    "width": 800,
    "height": 600,
    "data": { "url": "data/Meteorite_Landings.csv" },
    "mark": { "type": "line" },
    "encoding": {
        "x": { "timeUnit": "year", "field": "year", "type": "ordinal" },
        "y": { "aggregate": "count", "field": "id" }
    }
}